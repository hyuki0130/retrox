{
  "name": "retrox-mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:device": "react-native run-ios --device",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "e2e:build:ios": "detox build --configuration ios.sim.debug",
    "e2e:build:ios:release": "detox build --configuration ios.sim.release",
    "e2e:build:android": "detox build --configuration android.emu.debug",
    "e2e:test:ios": "detox test --configuration ios.sim.debug",
    "e2e:test:ios:release": "detox test --configuration ios.sim.release",
    "e2e:test:android": "detox test --configuration android.emu.debug",
    "e2e:ci:ios": "bash scripts/e2e-ci-ios.sh",
    "e2e:ci:ios:debug": "CONFIGURATION=ios.sim.debug bash scripts/e2e-ci-ios.sh",
    "e2e:build:ios:device": "detox build --configuration ios.device.release",
    "e2e:test:ios:device": "detox test --configuration ios.device.release"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@shopify/react-native-skia": "^1.0.0",
    "react": "18.2.0",
    "react-native": "0.73.4",
    "react-native-google-mobile-ads": "^12.4.0",
    "react-native-reanimated": "^3.6.3",
    "react-native-safe-area-context": "^4.14.1",
    "react-native-screens": "^3.29.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "^0.73.21",
    "@react-native/eslint-config": "^0.73.2",
    "@react-native/metro-config": "^0.73.5",
    "@react-native/typescript-config": "^0.73.1",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "detox": "^20.46.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "jest-circus": "^29.7.0",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
