{
  "name": "retrox-mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --port 9091",
    "ios": "react-native run-ios --port 9091",
    "ios:device": "react-native run-ios --port 9091 --device",
    "start": "react-native start --port 9091",
    "test": "jest",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "e2e:build:ios": "detox build --configuration ios.sim.debug",
    "e2e:build:ios:release": "detox build --configuration ios.sim.release",
    "e2e:build:android": "detox build --configuration android.emu.debug",
    "e2e:test:ios": "detox test --configuration ios.sim.debug",
    "e2e:test:ios:release": "detox test --configuration ios.sim.release",
    "e2e:test:android": "detox test --configuration android.emu.debug",
    "e2e:ci:ios": "bash scripts/e2e-ci-ios.sh",
    "e2e:ci:ios:debug": "CONFIGURATION=ios.sim.debug bash scripts/e2e-ci-ios.sh",
    "e2e:build:ios:device": "detox build --configuration ios.device.release",
    "e2e:test:ios:device": "detox test --configuration ios.device.release"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.9.1",
    "@react-navigation/native": "^7.1.27",
    "@react-navigation/native-stack": "^7.9.1",
    "@shopify/react-native-skia": "^2.4.14",
    "base64-js": "^1.5.1",
    "react": "19.2.0",
    "react-native": "0.83.1",
    "react-native-google-mobile-ads": "^13.6.1",
    "react-native-reanimated": "^4.2.1",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.19.0",
    "react-native-worklets": "^0.7.1",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/runtime": "^7.28.0",
    "@react-native-community/cli": "^20.1.0",
    "@react-native-community/cli-platform-android": "^20.1.0",
    "@react-native-community/cli-platform-ios": "^20.1.0",
    "@react-native/babel-preset": "^0.83.1",
    "@react-native/eslint-config": "^0.83.1",
    "@react-native/metro-config": "^0.83.1",
    "@react-native/typescript-config": "^0.83.1",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.2.0",
    "@types/react-test-renderer": "^19.1.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "detox": "^20.46.3",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "prettier": "^3.7.4",
    "react-test-renderer": "19.2.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18"
  }
}
